#include<stdio.h>

int  main()
{
    int n1,n2;
    int sum,subtract;
    
    float num1,num2;
    double sum1,subtract1;
    
    scanf("%d %d",&n1,&n2);
    
    scanf("%f%f",&num1,&num2);
    
    sum=n1+n2;
    subtract=n1-n2;
    
     printf("%d %d",sum,subtract);
     sum1=num1+num2;
    subtract1=num1-num2;
   
    printf("\n%.1f %.1f",sum1,subtract1);
return 0;
}
