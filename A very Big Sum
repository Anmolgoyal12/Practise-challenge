#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   long long int arr[10];
    int n,i;
    scanf("%d",&n);
        long long int sum=0;
    
    for(i=0;i<n;i++)
        scanf("%lld",&arr[i]);
     for(i=0;i<n;i++)
         {
         sum=sum+arr[i];
     }
    printf("%lld",sum);
    return 0;
}
